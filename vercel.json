{
  "version": 2,
  "cleanUrls": true,
  "routes": [
    { "handle": "filesystem" },
    {
      "src": "/assets/(.*)",
      "headers": {
        "Cache-Control": "public, max-age=31536000, immutable"
      }
    },
    {
      "src": "/sw\\.js",
      "headers": {
        "Cache-Control": "no-cache"
      }
    },
    { "src": "/(.*)", "dest": "/index.html" }
  ]
}
