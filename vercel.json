{
  "version": 2,
  "cleanUrls": true,
  "routes": [
    { "handle": "filesystem" },
    {
      "src": "/assets/(.*)",
      "headers": [
        { "key": "Cache-Control", "value": "public, max-age=31536000, immutable" }
      ]
    },
    {
      "src": "/sw\\.js",
      "headers": [
        { "key": "Cache-Control", "value": "no-cache" }
      ]
    },
    { "src": "/(.*)", "dest": "/index.html" }
  ]
}


